set(CMAKE_SYSTEM_NAME Windows)
set(CROSS_COMPILE ON)
set(CMAKE_SYSROOT /usr/x86_64-w64-mingw32/sys-root)
set(CMAKE_C_COMPILER x86_64-w64-mingw32-gcc)
set(CMAKE_CXX_COMPILER x86_64-w64-mingw32-g++)
set(CROSS_COMPILE_PREFIX x86_64-w64-mingw32-)
set(TARGET_NAME mingw64)
set(CMAKE_IGNORE_PATH /usr/inlclude)
set(CMAKE_FIND_ROOT_PATH_MODE_PROGRAM NEVER)
set(CMAKE_FIND_ROOT_PATH_MODE_LIBRARY ONLY)
set(CMAKE_FIND_ROOT_PATH_MODE_INCLUDE ONLY)
set(CMAKE_FIND_ROOT_PATH_MODE_PACKAGE ONLY)
set(STATIC_LIB_EXT dll.a)
set(COMPILE_OPT "-DCMAKE_SYSTEM_NAME=${CMAKE_SYSTEM_NAME}" "-DCMAKE_C_COMPILER=${CMAKE_C_COMPILER}" "-DCMAKE_SYSROOT=${CMAKE_SYSROOT}" "-DCMAKE_CXX_COMPILER=${CMAKE_CXX_COMPILER}")
set(COMPILE_FLAG "CC=${CMAKE_C_COMPILER}" "LDFLAGS=--sysroot=${CMAKE_SYSROOT} -L${CMAKE_SYSROOT}/mingw/lib" "CFLAGS=-isysroot=${CMAKE_SYSROOT} -I${CMAKE_SYSROOT}/mingw/include" "CXX_FLAGS=-isysroot=${CMAKE_SYSROOT} -I${CMAKE_SYSROOT}/mingw/include" "CXX=${CMAKE_CXX_COMPILER}")

